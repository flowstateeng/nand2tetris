/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=notsel);
    Nand(a=a, b=notsel, out=nandout1);
    Nand(a=b, b=sel, out=nandout2);
    Nand(a=nandout1, b=nandout2, out=out);
}
